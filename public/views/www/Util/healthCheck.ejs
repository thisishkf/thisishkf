<h3>health Check</h3>
<span>on <%= new Date()%></span>

<table class="table">
	<tr>
		<th>Site</th>
		<th>IP</th>
		<th>Status</th>
		<th>Response Time (ms)</th>
		<th>Message</th>
	</tr>
	<% var result = data %>
	<%for (keys in result) {%>
		<tr>
			<td>
				<%=keys%>
			</td>
			<td>
				<%=result[keys].ip%>
			</td>
			<td>
				<% if(result[keys].active){ %>
					<span class="badge badge-pill badge-success" style="width: 20px; height:20px"> </span>
					<%} else{ %>
						<span class="badge badge-pill badge-danger" style="width: 20px; height:20px"> </span>
						<%}%>
			</td>
			<td>
				<%=result[keys].responseTime%>
			</td>
			<td>
				<%=result[keys].msg%>
			</td>
		</tr>
	<%}%>
</table>
<script>
	setTimeout(function () {
		window.location.reload();
	}, 1800000);
</script>