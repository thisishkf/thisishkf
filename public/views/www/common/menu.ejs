<header>
	<nav class="navbar navbar-expand-lg navbar-light bg-light">
		<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
		 aria-expanded="false" aria-label="Toggle navigation">
			<span class="navbar-toggler-icon"></span>
		</button>
		<a class="navbar-brand" href="#">
			<img src="/static/images/logo.png" width="30" height="30" alt="LOGO HERE">
		</a>
		<div class="collapse navbar-collapse" id="navbarSupportedContent">
			<ul class="navbar-nav mr-auto menu_container">


				<% for (let category in menu) { %>
					<li class="dropdown">
						<a class="dropdown-toggle" data-toggle="dropdown"><%=category %></a>
						<ul class="dropdown-menu">
							<% for(let menuItem of menu[category]){ %>
								<% if(menuItem.status == "active") { %>
									<li class="nav-item">
										<a class="nav-link" href="<%=menuItem.uri%>"><%=menuItem.title%></a>
									</li>
								<% } %>
							<% } %>
						</ul>
					</li>
				<% } %>
			</ul>
		</div>
	</nav>
</header>